function _setModelState(orbital) {

  measures delete;
  select *; set measures nanometers;
  font measures 15.0 SansSerif Plain;
  mo cutoff 0.05;
  mo 10;
  mo noFill mesh noDots frontOnly frontlit;

  boundBox off;
  font boundBox 14.0 SansSerif Plain;
  boundBox off;

  frank on;
  font frank 16.0 SansSerif Bold;
  select *;

}

