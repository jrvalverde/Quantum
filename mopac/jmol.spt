function _setState(num_frames) {
  initialize;
  set refreshing false;
  _setWindowState;
  for (var i = 1; i <= num_frames; i = i+1)
    var filename = ("00000"+i)[-4][0]+".mgf"
    _setFileState(@filename);
    _setVariableState;
    _setModelState(10);
    _setPerspectiveState;
    _setSelectionState;
    set refreshing true;
    set antialiasDisplay false;
    set antialiasTranslucent true;
    set antialiasImages true;
    var filename = "images/"+("00000"+i)[-4][0]+".jpg"
    write IMAGE 800 600 JPG @filename
  end for
}

